{
  "name": "simplybook_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preview": "npm-run-all --parallel sync start build:css:watch",
    "sync": "browser-sync start -p 'localhost:3000' --files '**/*.php' 'build/*.js' 'build/*.css'",
    "sync-rogier": "browser-sync start -p 'localhost:3000' --files '**/*.php' 'build/*.js' 'build/*.css'",
    "start": "set \"INLINE_RUNTIME_CHUNK=false\" && export BURST_ENV=development && wp-scripts start",
    "build": "set \"INLINE_RUNTIME_CHUNK=false\" && export BURST_ENV=production && wp-scripts build",
    "format": "prettier --write '**/*.js' '**/*.css' '**/*.html'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "build:css": "postcss src/tailwind.css -o src/tailwind.generated.css",
    "build:css:watch": "postcss src/tailwind.css -o src/tailwind.generated.css --watch"
  },
  "keywords": [],
