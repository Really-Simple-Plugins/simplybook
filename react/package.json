{
  "name": "simplybook_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preview": "npm-run-all --parallel sync start",
    "sync": "browser-sync start -p 'https://simplybook.local' --files '**/*.php' '**/*.jsx' '**/*.tsx' 'build/*.js' 'build/*.css' build:css",
    "sync-rogier": "browser-sync start -p 'https://simplybook.local' --files '**/*.php' '**/*.jsx' '**/*.tsx' 'build/*.js' 'build/*.css' build:css",
    "start": "set \"INLINE_RUNTIME_CHUNK=false\" && export PLUGIN_ENV=development && wp-scripts start",
    "build": "set \"INLINE_RUNTIME_CHUNK=false\" && export PLUGIN_ENV=production && wp-scripts build",
    "format": "prettier --write '**/*.js' '**/*.css' '**/*.html'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "build:css": "postcss src/tailwind.css -o src/tailwind.generated.css",
    "build:css:watch": "postcss src/tailwind.css -o src/tailwind.generated.css --watch",
    "postinstall": "node fix-chalk.js"
  },
